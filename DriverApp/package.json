{
	"name": "ouka-app",
	"version": "0.9.4",
	"scripts": {
		"dev": "cross-env VITE_ENV_TYPE=dev vite",
		"dev:test": "cross-env VITE_ENV_TYPE=test vite",
		"dev:prod": "cross-env VITE_ENV_TYPE=prod vite",
		"build": "cross-env VITE_ENV_TYPE=prod vite build",
		"build:dev": "npm run typecheck`                                                                                                                     && cross-env VITE_ENV_TYPE=dev vite build",
		"build:test": "npm run typecheck && cross-env VITE_ENV_TYPE=test vite build",
		"build:vercel": "cross-env VITE_HASH_ROUTE=true vite build",
		"preview": "vite preview --port 5050",
		"typecheck": "vue-tsc --noEmit",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
		"postinstall": "patch-package",
		"release": "standard-version",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
	},
	"lint-staged": {
		"*.{vue,js,jsx,ts,tsx}": "eslint --fix"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-customizable"
		}
	},
	"dependencies": {
		"@better-scroll/core": "^2.4.2",
		"@vant/compat": "^1",
		"@vant/touch-emulator": "^1.4.0",
		"@vant/use": "^1.6.0",
		"@vueuse/core": "^8.5.0",
		"axios": "^1.6.5",
		"clipboard": "^2.0.11",
		"colord": "^2.9.2",
		"crypto-js": "^4.1.1",
		"dayjs": "^1.11.2",
		"echarts": "^5.3.2",
		"form-data": "^4.0.0",
		"lodash-es": "^4.17.21",
		"moment": "^2.30.1",
		"pinia": "^2.0.36",
		"print-js": "^1.6.0",
		"qs": "^6.10.3",
		"rollup": "^2.50.0",
		"swiper": "^8.1.6",
		"ua-parser-js": "^1.0.2",
		"vant": "^4.8.2",
		"vditor": "^3.8.15",
		"volar": "^0.0.2",
		"vue": "3.2.37",
		"vue-pdf-embed": "^1.1.5",
		"vue-router": "^4.0.15",
		"vue3-pdfjs": "^0.1.6",
		"xgplayer": "^2.31.6",
		"youtube-player": "^5.5.2"
	},
	"devDependencies": {
		"@amap/amap-jsapi-types": "^0.0.8",
		"@commitlint/cli": "^16.0.0",
		"@commitlint/config-conventional": "^17.0.0",
		"@iconify/json": "^2.1.50",
		"@iconify/vue": "^3.2.1",
		"@types/bmapgl": "^0.0.5",
		"@types/crypto-js": "^4.1.1",
		"@types/node": "^17.0.35",
		"@types/qs": "^6.9.7",
		"@types/ua-parser-js": "^0.7.36",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^5.26.0",
		"@vitejs/plugin-vue": "^3.0.0",
		"@vitejs/plugin-vue-jsx": "^1.3.10",
		"@vue/eslint-config-prettier": "^7.0.0",
		"@vue/eslint-config-typescript": "^10.0.0",
		"commitizen": "^4.0.0",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^2.0.0",
		"cz-customizable": "^6.3.0",
		"eslint": "^8.16.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.7.1",
		"lint-staged": "^12.4.2",
		"patch-package": "^6.4.7",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "^2.6.2",
		"rollup-plugin-visualizer": "^5.6.0",
		"sass": "^1.69.7",
		"standard-version": "^9.5.0",
		"typescript": "^4.6.4",
		"unocss": "^0.58.3",
		"unplugin-icons": "^0.14.3",
		"unplugin-vue-components": "0.26.0",
		"unplugin-vue-define-options": "^0.6.1",
		"vite": "^3.0.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-html": "^3.2.0",
		"vue-tsc": "^0.35.0",
		"vueuc": "^0.4.37"
	}
}
